{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "lib": ["ES2022"],
    // "moduleResolution": "NodeNext",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "typeRoots": ["./types", "./node_modules/@girs"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
